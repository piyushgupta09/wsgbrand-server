{
    "name": "fpaipl/wsgbrand",
    "type": "project",
    "description": "Brand Management System",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.4",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "minishlink/web-push": "^8.0",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.5",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-permission": "^6.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Fpaipl\\Authy\\": "modules/authy/src/",
            "Fpaipl\\Authy\\Database\\Seeders\\": "modules/authy/database/seeders/",
            "Fpaipl\\Panel\\": "modules/panel/src/",
            "Fpaipl\\Panel\\Database\\Seeders\\": "modules/panel/database/seeders/",
            "Fpaipl\\Prody\\": "modules/prody/src/",
            "Fpaipl\\Prody\\Database\\Seeders\\": "modules/prody/database/seeders/",
            "Fpaipl\\Brandy\\": "modules/brandy/src/",
            "Fpaipl\\Brandy\\Database\\Seeders\\": "modules/brandy/database/seeders/"
        },
        "files": [
            "modules/panel/helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Fpaipl\\Authy\\" : "modules/authy/src/",
            "Fpaipl\\Panel\\" : "modules/panel/src/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
